{
  "name": "task-serializer",
  "version": "2.1.0",
  "description": "Serialize tasks/promises for integrated control. Option for limiting number of concurrent tasks.",
  "main": "dist/index.js",
  "files": [
    "dist/*.js","dist/*.d.ts",
    "tests-js/demo-*.js",
    "scripts-ts/post-install-demo.js",
    "scripts-ts/demo-filenames.js",
    "scripts-ts/gen-examples.js",
    "tests-ts/demo-async-iter.ts",
    "tests-ts/demo-callbacks.ts",
    "tests-ts/demo-lib.ts",
    "tests-ts/demo-next-symbol.ts",
    "tests-ts/demo-wait-all.ts",
    "scripts/post-install-demo.sh",
    "scripts/post-install-demo-jsonly.sh" 
  ],
  "scripts": {
    "test": "node ./tests-js/script-test.js;node ./tests-ts/script-test.js",
    "pre-publish": "node scripts-ts/pre-publish-test.js ./tests-js ./examples-js ./tests-ts ./examples-ts",

    "build-dist": "tsc",
    "build-tests": "tsc -p tsconfig.tests.json",
    "build-all": "npm run build-dist; npm run build-tests",
    "clean-dist": "rm dist/*",
    "clean-tests": "bash -c 'rm -f tests-ts/*.{js,js.map,d.ts}; rm -f scripts-ts/*.{js,js.map,d.ts}' ; rm -f dist-*/*; rmdir dist-* 2>/dev/null || true ; ",
    "clean-all": "npm run clean-dist; npm run clean-tests"
  },
  "engines": {
    "node": ">=12.9"
  },
  "keywords": [
    "top down",
    "serializer",
    "serialize events",
    "serialize tasks",
    "concurrent task limit"
  ],
  "author": "craigphicks",
  "license": "ISC",
  "devDependencies": {
    "@tsconfig/node10": "^1.0.7",
    "@tsconfig/node12": "^1.0.7",
    "@tsconfig/node14": "^1.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@types/node": "^14.14.14",
    "mini-preproc": "^1.1.2",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
