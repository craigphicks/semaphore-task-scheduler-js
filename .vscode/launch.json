{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
      
    {
      "name": "gen-examples",
      "program": "${workspaceFolder}/scripts-ts/gen-examples.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "args": ["./tests-js","./tests-ts","./examples-js","./examples-ts"],
      "type": "pwa-node"
    },
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
        {
            "type": "node",
            "request": "launch",
            "name": "current open js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
    ]
}